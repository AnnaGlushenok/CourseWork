<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="278.0" stylesheets="@deleteRole.css" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.scenes.AdminMenu.DeleteRole.DeleteRoleController">
   <Button fx:id="deleteBtn" focusTraversable="false" layoutX="60.0" layoutY="98.0" mnemonicParsing="false"
           onAction="#delete" prefHeight="30.0" prefWidth="150.0" text="Удалить роль" textFill="WHITE">
      <font>
         <Font name="System Bold" size="14.0"/>
      </font>
   </Button>
   <ComboBox fx:id="rolesList" editable="true" focusTraversable="false" layoutX="60.0" layoutY="50.0" prefWidth="150.0"
             promptText="Роль"/>
</AnchorPane>
